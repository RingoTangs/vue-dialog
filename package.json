    "@vue/babel-plugin-jsx": "^1.2.5",
    "autoprefixer": "^10.4.20",
    "core-js": "3",
    "cross-env": "^7.0.3",
    "eslint": "^9.17.0",
    "eslint-plugin-format": "^1.0.1",
    "postcss": "^8.5.3",
    "regenerator-runtime": "^0.14.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.29.1",
    "rollup-plugin-postcss": "^4.0.2",
    "typescript": "^5.7.2",
    "vue": "3.3.4"
  }
